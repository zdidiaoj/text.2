//模拟函数库函数：strcpy
char * strcpy(char *dst, const char *src)
{
	char *cp = dst;
	assert(dst&&src);
	while (*cp++ = *src++)
		;
	return (dst);
}
int main()
{
	char arr[] = "abcdef";
	char arr1[] = { 0 };
	strcpy(arr1, arr);
	printf("%s\n", arr1);
	return 0;
}

